		class LOG_DO
			inherit DATA_OBJECT
			feature
			time: TIME
			table: INTEGER
			cpr: CPRNO
			activity: Activity
			
			effective fullyInitialized : BOOLEAN 
			effective UpdateValues -> dummy : DATA_OBJECT
		end
		
		class Activity
			feature
			value: CHARACTER
			
			invariant
			value element_of { 'W', 'R' }
		end
