<NotepadPlus>
    <UserLang name="BON" ext="bon">
        <Settings>
            <Global caseIgnored="no" />
            <TreatAsSymbol comment="no" commentLine="yes" />
            <Prefix words1="no" words2="no" words3="no" words4="no" />
        </Settings>
        <KeywordLists>
            <Keywords name="Delimiters">&quot;00&quot;00</Keywords>
            <Keywords name="Folder+">system_chart cluster_chart class_chart static_diagram dynamic_diagram cluster action scenario_chart event_chart creation_chart</Keywords>
            <Keywords name="Folder-">end</Keywords>
            <Keywords name="Operators">, : ;</Keywords>
            <Keywords name="Comment">1 2 0--</Keywords>
            <Keywords name="Words1">indexing cluster class explanation query command constraint part scenario object description component inherit incoming outgoing event involves creator creates client calls deferred reused redefined feature invariant require ensure</Keywords>
            <Keywords name="Words2">author created keywords in_cluster created revised</Keywords>
            <Keywords name="Words3">ANY NONE BOOLEAN INTEGER REAL VALUE NUMERIC COMMAND TIME DATE UNIT AMOUNT STATE TRANSITION FUNCTION SET SEQUENCE SORTED_LIST TABLE STACK QUEUE</Keywords>
            <Keywords name="Words4">not and or xor exists for_all such_that it_holds member_of delta old Result Current Void make</Keywords>
        </KeywordLists>
        <Styles>
            <WordsStyle name="DEFAULT" styleID="11" fgColor="808080" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="FOLDEROPEN" styleID="12" fgColor="800000" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="FOLDERCLOSE" styleID="13" fgColor="800000" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD1" styleID="5" fgColor="0000FF" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="KEYWORD2" styleID="6" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="3" />
            <WordsStyle name="KEYWORD3" styleID="7" fgColor="00A600" bgColor="FFFFFF" fontName="" fontStyle="3" />
            <WordsStyle name="KEYWORD4" styleID="8" fgColor="0080FF" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT" styleID="1" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="COMMENT LINE" styleID="2" fgColor="00B02D" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="NUMBER" styleID="4" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="OPERATOR" styleID="10" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="1" />
            <WordsStyle name="DELIMINER1" styleID="14" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER2" styleID="15" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
            <WordsStyle name="DELIMINER3" styleID="16" fgColor="000000" bgColor="FFFFFF" fontName="" fontStyle="0" />
        </Styles>
    </UserLang>
</NotepadPlus>
